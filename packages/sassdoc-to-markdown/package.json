{
  "name": "@hidoo/sassdoc-to-markdown",
  "version": "0.7.0",
  "description": "Generate markdown document use SassDoc.",
  "main": "lib/index.js",
  "bin": {
    "sassdoc2md": "bin/index.js"
  },
  "files": [
    "bin/",
    "lib/",
    "template/"
  ],
  "scripts": {
    "dev": "babel-node ./src/cli.js",
    "prebuild": "rimraf ./lib",
    "build": "babel ./src --out-dir ./lib",
    "test": "cross-env NODE_ENV=test mocha"
  },
  "keywords": [
    "sassdoc",
    "markdown",
    "documentation"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "hidoo",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "handlebars": "4.7.7",
    "mdast-util-inject": "1.1.0",
    "meow": "9.0.0",
    "remark": "13.0.0",
    "sassdoc": "2.7.3"
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.16.7",
    "@babel/node": "7.16.8",
    "@babel/preset-env": "7.16.8",
    "@babel/register": "7.16.9",
    "babel-preset-power-assert": "3.0.0",
    "mocha": "9.1.4",
    "power-assert": "1.6.1",
    "rimraf": "3.0.2"
  }
}
