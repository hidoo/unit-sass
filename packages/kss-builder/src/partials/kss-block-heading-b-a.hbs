<header class="kss-block-heading-b-a{{#if modifier_class}}  {{modifier_class}}{{/if}}  kss-unit-box{{#if this.utils}}  {{this.utils}}{{/if}}">
  <div class="kss-block-heading-b-a__container  kss-unit-box__container">
    <h1 class="kss-block-heading-b-a__name  kss-unit-text  kss-unit-text--theme-a-a-e  kss-unit-text--truncate-true">{{packageName}}</h1>
    <p class="kss-block-heading-b-a__version  kss-unit-text  kss-unit-text--theme-a-a-b">v{{packageVersion}}</p>
    <p class="kss-block-heading-b-a__description  kss-unit-text  kss-unit-text--theme-a-a-a">{{packageDescription}}</p>
  </div>
</header>
